{
  "version": 2,
  "framework": null,
  "buildCommand": "cd \"thursday thrift project\" && chmod +x vercel-build.sh && ./vercel-build.sh",
  "outputDirectory": "thursday thrift project/build/web",
  "public": true,
  "cleanUrls": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
} 